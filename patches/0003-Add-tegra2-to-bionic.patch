From b5f665af94dbb0f0e62bfd4bc675e47a59aac410 Mon Sep 17 00:00:00 2001
From: kasperhettinga <kasper.hettinga@gmail.com>
Date: Mon, 30 Dec 2013 07:58:19 +0100
Subject: [PATCH] Add tegra2 to bionic

Thanks to AAccount!

Change-Id: I6290f50aafebacc23365c3aaa8d01d6c1b54e01e
---
 libc/Android.mk                      |   8 +-
 1 files changed, 8 insertions(+), 1 deletion(-)
 
diff --git a/libc/Android.mk b/libc/Android.mk
index 9fc94df..8286240 100644
--- a/libc/Android.mk
+++ b/libc/Android.mk
@@ -181,6 +181,13 @@ libc_common_src_files := \
 	netbsd/nameser/ns_print.c \
 	netbsd/nameser/ns_samedomain.c \
 
+# cortex-a9 without neon
+ifneq ($(TARGET_CPU_VARIANT),tegra2)
+    libc_common_src_files += \
+        bionic/memchr.c \
+
+endif
+
 # Fortify implementations of libc functions.
 libc_common_src_files += \
     bionic/__fgets_chk.cpp \
@@ -369,7 +376,6 @@ libc_common_src_files += \
 	string/strncpy.c \
 	bionic/strchr.cpp \
 	string/strrchr.c \
-	bionic/memchr.c \
 	bionic/memrchr.c \
 	string/index.c \
 	bionic/strnlen.c \
